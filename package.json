{
	"name": "github-issues-as-headless-cms",
	"description": "Use Github Issues as a headless CMS",
	"scripts":
  {
		"start": "ELEVENTY_ENV=development npx @11ty/eleventy --serve",
		"build": "ELEVENTY_ENV=production npx @11ty/eleventy",
		"clean:cache": "rm -rf .cache && yarn eleventy",
		"clean:build": "rm -rf _site",
		"clean": "yarn run clean:build && yarn run clean:cache"
	},
	"license": "MIT",
	"devDependencies": {
		"@11ty/eleventy": "^1.0.0",
		"@11ty/eleventy-cache-assets": "^2.3.0",
		"@11ty/eleventy-plugin-rss": "^1.1.2",
		"clean-css": "^5.2.2",
		"cross-env": "^7.0.3",
		"dotenv": "^14.2.0",
		"html-minifier": "^4.0.0",
		"jsdom": "^19.0.0",
		"luxon": "^2.3.0",
		"markdown-it": "^12.3.2",
		"probe-image-size": "^7.2.2",
		"slugify": "^1.6.5"
	},
	"dependencies": {
		"all": "0.0.0"
	}
}
